{
  "fmt": {
    "semiColons": true,
    "indentWidth": 2,
    "singleQuote": true,

    "exclude": ["node_modules", ".git", "app/routeTree.gen.ts"]
  },
  "tasks": {
    "dev": "deno run --watch main.ts",
    "devv": "deno run -A npm:vinxi dev",
    "build": "deno run npm:vinxi build",
    "start": "deno run npm:vinxi start",
    "test": "vitest"
  },
  "type": "module",
  "compilerOptions": {
    "lib": [
      "ESNext",
      "ES2020",
      "DOM",
      "DOM.Iterable",
      "deno.window",
      "deno.ns",
      "DOM.AsyncIterable",
      "webworker"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "jsxImportSourceTypes": "@types/react"
  },
  "nodeModulesDir": "auto",

  "imports": {
    "@deno-plc/vite-plugin-deno": "jsr:@deno-plc/vite-plugin-deno@^2.1.9",
    "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.0",
    "@std/assert": "jsr:@std/assert@1",
    "@tanstack/react-query": "npm:@tanstack/react-query@^5.62.7",
    "@tanstack/react-router": "npm:@tanstack/react-router@1.87.7",
    "@tanstack/router-devtools": "npm:@tanstack/router-devtools@1.87.7",
    "@tanstack/router-plugin": "npm:@tanstack/router-plugin@1.87.7",
    "@tanstack/start": "npm:@tanstack/start@1.87.7",
    "@types/react": "npm:@types/react@^18.3.11",
    "@types/react-dom": "npm:@types/react-dom@^18.3.1",
    "@vitejs/plugin-react": "npm:@vitejs/plugin-react@^4.3.4",
    "jsdom": "npm:jsdom@^25.0.1",
    "react": "npm:react@^18.3.1",
    "react-dom": "npm:react-dom@^18.3.1",
    "vinxi": "npm:vinxi@^0.5.1",
    "vite": "npm:vite@^6.0.0",
    "vitest": "npm:vitest@^2.1.8"
  }
}
